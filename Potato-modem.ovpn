# OpenVPN Build
# Potato OHP
# Telegram : @aldi_nc
# Instagram : @aldi_nc

setenv CLIENT_CERT 0
setenv opt block-outside-dns
client
dev tun
proto tcp
remote "your bug" 1194
persist-tun
persist-key
persist-remote-ip
comp-lzo
reneg-sec 0
pull
setenv FRIENDLY_NAME "Potato OHP + OVPN For Modem"
resolv-retry infinite
remote-cert-tls server
verb 3
auth-user-pass
cipher none
auth none
auth-nocache
script-security 2
http-proxy aldiblues 9088

